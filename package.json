{
  "name": "elm-ui",
  "version": "0.2.2",
  "description": "UI Libary in Elm",
  "scripts": {
    "start": "bin/index.js server",
    "deploy": "gulp deploy",
    "predeploy": "npm run build",
    "build": "bin/index.js build",
    "test": "nightwatch test2/tests -e chrome,firefox",
    "elm-install": "bash elm-install.sh",
    "elm-docs": "elm-make --docs=documentation.json",
    "spec-sauce": "intern-runner config=specs/config-sauce",
    "spec-local": "intern-runner config=specs/config-local",
    "selenium": "selenium-standalone install; selenium-standalone start"
  },
  "homepage": "https://github.com/gdotdesign/elm-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gdotdesign/elm-ui.git"
  },
  "author": "",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/gdotdesign/elm-ui/issues"
  },
  "optionalDependencies": {
    "pty.js": "0.3.0"
  },
  "dependencies": {
    "elm-github-install": "0.1.0",
    "gulp-gh-pages": "0.5.4",
    "browser-sync": "2.12.8",
    "autoprefixer": "6.3.6",
    "koa-static": "2.0.0",
    "koa-router": "5.4.0",
    "underscore": "1.8.3",
    "nightwatch": "*",
    "node-sass": "3.7.0",
    "clean-css": "3.4.12",
    "commander": "2.9.0",
    "uglify-js": "2.6.2",
    "request": "2.74.0",
    "ansi_up": "1.3.0",
    "async": "1.5.2",
    "gulp": "3.9.1",
    "elm": "0.17.1",
    "koa": "1.2.0",
    "ncp": "2.0.0"
  },
  "devDependencies": {
    "selenium-standalone": "5.1.0",
    "intern": "3.2.0",
    "dotenv": "2.0.0"
  },
  "preferGlobal": true,
  "bin": {
    "elm-ui": "bin/index.js"
  },
  "engines": {
    "node": ">= 4"
  }
}
