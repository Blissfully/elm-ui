{
  "version": "0.2.0",
  "summary": "UI Library in ELM",
  "repository": "https://github.com/gdotdesign/elm-ui.git",
  "license": "BSD-2-Clause",
  "source-directories": [
    "source",
    "tests",
    "vendor"
  ],
  "native-modules": true,
  "exposed-modules": [
    "Ext.Color",
    "Ext.Number",
    "Ext.Date",
    "Html.Events.Geometry",
    "Html.Events.Options",
    "Html.Events.Extra",
    "Ui.Helpers.Drag",
    "Ui.Helpers.Dropdown",
    "Ui.Helpers.Emitter",
    "Ui.Helpers.Intendable",
    "Ui.Native.Browser",
    "Ui.Native.Dom",
    "Ui.Utils.Env",
    "Ui.App",
    "Ui.Button",
    "Ui.ButtonGroup",
    "Ui.Calendar",
    "Ui.Checkbox",
    "Ui.ColorPanel",
    "Ui.ColorPicker",
    "Ui.Container",
    "Ui.DatePicker",
    "Ui.DropdownMenu",
    "Ui.IconButton",
    "Ui.Image",
    "Ui.InplaceInput",
    "Ui.Input",
    "Ui.Loader",
    "Ui.Modal",
    "Ui"
  ],
  "dependencies": {
    "elm-lang/virtual-dom": "1.0.0 <= v < 2.0.0",
    "elm-lang/mouse": "1.0.0 <= v < 2.0.0",
    "elm-lang/html": "1.0.0 <= v < 2.0.0",
    "elm-lang/core": "4.0.0 <= v < 5.0.0"
  },
  "elm-version": "0.17.0 <= v < 0.18.0"
}
